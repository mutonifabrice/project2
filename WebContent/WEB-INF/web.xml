<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
    <display-name>Practice</display-name>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.auca.service.HelloWorld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>forgetPassword</servlet-name>
        <servlet-class>com.auca.service.ForgetPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>forgetPassword</servlet-name>
        <url-pattern>/forgetPassword</url-pattern>
    </servlet-mapping>
<servlet>
<servlet-name>SignUP</servlet-name>
<servlet-class>com.auca.service.Sign_Up</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>SignUP</servlet-name>
<url-pattern>/signupform</url-pattern>
</servlet-mapping>

    <servlet>
        <servlet-name>fileUploadServlet</servlet-name>
        <servlet-class>com.auca.service.StudentAdmission</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fileUploadServlet</servlet-name>
        <url-pattern>/studentUploadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>emailServlet</servlet-name>
        <servlet-class>com.auca.service.EmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>emailServlet</servlet-name>
        <url-pattern>/sendConfirmationEmail</url-pattern>
    </servlet-mapping>

 
</web-app>
